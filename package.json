{
  "name": "@efrem/showcommits",
  "version": "1.1.0",
  "main": "dist/index.js",
  "repository": "https://github.com/MrEfrem/showcommits.git",
  "author": "MrEfrem <mr.efrem@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node -r @babel/register src/index.js",
    "outdated": "yarn upgrade-interactive",
    "lint": "eslint src/**/*.js",
    "build": "babel src -d dist",
    "_prepublish": "yarn lint && yarn tsc && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@types/core-js": "2.5.3",
    "@types/node": "^13.13.2",
    "@yarnpkg/pnpify": "^2.0.0-rc.21",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "bin": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint && yarn tsc",
      "pre-push": "yarn lint && yarn tsc"
    }
  },
  "dependencies": {
    "core-js": "^3.6.5"
  }
}
